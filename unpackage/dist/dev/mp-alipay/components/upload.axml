<view class="upload-box data-v-51e26630"><block a:for="{{files}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{files.length>0}}"><view class="upload-img-box relative data-v-51e26630"><image class="img data-v-51e26630" src="{{item.url}}"></image><image class="delete-icon data-v-51e26630" src="/static/icon/del.png" data-event-opts="{{[['tap',[['deleteItem',[index]]]]]}}" onTap="__e"></image><block a:if="{{item.pct=='wait'}}"><view class="fail data-v-51e26630">图片上传中...</view></block><block a:if="{{item.pct=='fail'}}"><view class="fail data-v-51e26630">图片上传失败</view></block></view></block></block><block a:if="{{files.length<9}}"><view data-event-opts="{{[['tap',[['uploadAll',['$event']]]]]}}" class="upload-img-box data-v-51e26630" onTap="__e"><image src="/static/icon/upload_bg.png" mode class="data-v-51e26630"></image></view></block></view>