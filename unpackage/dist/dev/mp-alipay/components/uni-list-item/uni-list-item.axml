<view class="{{(('uni-list-cell')+' '+(disabled===true||disabled==='true'?'uni-list-cell--disabled':''))}}" hover-class="{{disabled===true||disabled==='true'||showSwitch===true||showSwitch==='true'?'':'uni-list-cell--hover'}}" data-event-opts="{{[['tap',[['onClick',['$event']]]]]}}" onTap="__e"><view class="uni-list-cell__container"><block a:if="{{thumb}}"><view class="uni-list-cell__icon"><image class="uni-list-cell__icon-img" src="{{thumb}}"></image></view></block><block a:else><block a:if="{{showExtraIcon===true||showExtraIcon==='true'}}"><view class="uni-list-cell__icon"><uni-icon vue-id="1" color="{{extraIcon.color}}" size="{{extraIcon.size}}" type="{{extraIcon.type}}" onVueInit="__l"></uni-icon></view></block></block><view class="uni-list-cell__content"><view class="uni-list-cell__content-title">{{title}}</view><block a:if="{{note}}"><view class="uni-list-cell__content-note">{{note}}</view></block></view><block a:if="{{showBadge===true||showBadge==='true'||showArrow===true||showArrow==='true'||showSwitch===true||showSwitch==='true'}}"><view class="uni-list-cell__extra"><block a:if="{{showBadge===true||showBadge==='true'}}"><uni-badge vue-id="2" type="{{badgeType}}" text="{{badgeText}}" onVueInit="__l"></uni-badge></block><block a:if="{{showSwitch===true||showSwitch==='true'}}"><switch disabled="{{disabled}}" checked="{{switchChecked}}" data-event-opts="{{[['change',[['onSwitchChange',['$event']]]]]}}" onChange="__e"></switch></block><block a:if="{{showArrow===true||showArrow==='true'}}"><uni-icon vue-id="3" color="#bbb" size="20" type="arrowright" onVueInit="__l"></uni-icon></block></view></block></view></view>