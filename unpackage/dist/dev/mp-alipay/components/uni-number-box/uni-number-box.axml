<view class="uni-numbox"><view data-event-opts="{{[['tap',[['_calcValue',['subtract']]]]]}}" class="{{(('uni-numbox__minus')+' '+((disableSubtract||disabled)?'uni-numbox--disabled':''))}}" onTap="__e">-</view><input class="uni-numbox__value" type="number" disabled="{{disabled}}" data-event-opts="{{[['blur',[['_onBlur',['$event']]]]]}}" value="{{inputValue}}" onBlur="__e"/><view data-event-opts="{{[['tap',[['_calcValue',['add']]]]]}}" class="{{(('uni-numbox__plus')+' '+((disableAdd||disabled)?'uni-numbox--disabled':''))}}" onTap="__e">+</view></view>